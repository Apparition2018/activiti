    <startEvent id="start"/>
    <userTask id="userTask1" name="费用报销申请" activiti:assignee="张三"/>
    <exclusiveGateway id="eg"/>
    <userTask id="userTask2" name="审批【财务】" activiti:assignee="财务"/>
    <userTask id="userTask3" name="审批【部门经理】" activiti:assignee="部门经理"/>
